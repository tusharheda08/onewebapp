currentBuild.displayName="Hello-World-Project-#"+currentBuild.number

properties([[$class: 'JiraProjectProperty'], 
            parameters([choice(choices: ['master', 'future', 'development', 'testing'], 
            description: 'select branch to build project', name: 'branchName')])])

pipeline {
   agent any

   stages {
      stage("Git Checkout") {
         steps {
          echo "pulling changes from the branch ${params.branch}"
          git url:'https://github.com/tusharheda08/onewebapp.git/',branch:"${params.branch}"
           // git 'https://github.com/tusharheda08/onewebapp'
            
         }
      }
       
    }
 }
